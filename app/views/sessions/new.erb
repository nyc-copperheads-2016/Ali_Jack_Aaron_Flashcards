<h1>Make a new user</h1>
<% if defined?(errors) %>
  <% errors.each do |error| %>
  <%= error %><br>
  <% end %>
<% end %>
<form method='post' action='/sessions/create'>
  <input type='text' name='user[username]' />
  <input type='password' name='user[password]' />
  <input type='submit' />
</form>
